        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.2.0",
        "validator": "^13.9.0"
    },
    "devDependencies": {
        "concurrently": "^8.0.1",
        "nodemon": "^2.0.22",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
